{
  "name": "the-follower",
  "version": "1.0.0",
  "scripts": {
    "preingest": "rm -rf /tmp/the-release/db.sqlite",
    "ingest": "ts-node --project ./tsconfig.ts-node.json src/backend/ingest.ts",
    "dev": "next -p 3000",
    "predev": "yarn cleanup && yarn ingest && yarn",
    "prebuild": "yarn cleanup && yarn ingest",
    "build": "next build && next export -o ./dist/out",
    "cleanup": "rm -rf dist/",
    "serve": "serve -p 3000 ./dist/out"
  },
  "dependencies": {
    "next": "^9.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^4.4.1",
    "styled-icons": "^9.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@types/cheerio": "^0.22.21",
    "@types/glob": "^7.1.3",
    "@types/marked": "^1.1.0",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/sharp": "^0.25.1",
    "@types/styled-components": "^4.4.0",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cheerio": "^1.0.0-rc.3",
    "circular-dependency-plugin": "^5.2.0",
