    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "^7.14.2",
    "marked": "^1.1.1",
    "prettier": "1.19.1",
    "reading-time": "^1.2.0",
    "serve": "^11.0.0",
    "sharp": "^0.25.4",
    "slugify": "^1.4.4",
    "sqlite3": "^5.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "title-case": "^3.0.2",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.25",
    "typescript": "3.7.3"
  }
